  "dependencies": {
    "classnames": "^2.2.5",
    "postcss": "^5.2.10",
    "postcss-cli": "^2.6.0",
    "react": "^15.4.2",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
