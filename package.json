    "html-webpack-plugin": "^2.26.0",
    "node-sass": "^4.3.0",
    "postcss-loader": "^1.2.2",
    "pre-commit": "^1.2.2",
    "precss": "^1.4.0",
    "react-dom": "^15.4.2",
    "redux-logger": "^2.7.4",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
